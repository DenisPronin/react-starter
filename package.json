{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Simple starter kit for react/redux applications.",
  "engines": {
    "node": ">=5.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node server/server.js"
    },
    "build": {
      "command": "babel-node webpack/webpack.compiler.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenisPronin/react-starter.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "boilerplate",
    "starterkit"
  ],
  "author": "DenisPronin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DenisPronin/react-starter/issues"
  },
  "homepage": "https://github.com/DenisPronin/react-starter#readme",
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "0.0.8",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "node-sass": "^3.6.0",
    "postcss-loader": "^0.8.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
